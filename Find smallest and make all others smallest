#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
        // your code goes here
        int smallest=a[0];
       for(int i=0; i<n;i++)
       {
           if(a[i]<smallest)
           smallest = a[i];
       }
       int counter = 0;
       for(int i=0; i<n;i++)
       {
           if(a[i]!=smallest)
          
           counter++;
       }
       cout<<counter<< endl;
    }

}

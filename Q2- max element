//FInd the largest element

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin >> T;
	
	while(T--)
	{
	    int N;
	    cin >> N;
	    
	    vector<int> A(N);
	    for(int i =0; i<N; i++)
	    {
	        cin >> A[i];
	    }
	    
	    int largest = A[0];
	    for(int i=0; i<N;i++)
	    {
	        if(A[i] > largest)
	        {
	            largest = A[i];
	        }
	    }
	    
	    cout << largest << endl;
	}
	return 0;
}
